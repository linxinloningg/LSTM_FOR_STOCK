{
  "data": {
    "columns": [
      "close",
      "volume"
    ],
    "sequence_length": 15,
    "train_test_split": 0.85,
    "normalise": true
  },
  "training": {
    "epochs": 32,
    "batch_size": 64,
    "verbose": 1,
    "shuffle": "False",
    "validation_freq": 1
  },
  "model": {
    "loss": "mse",
    "optimizer": "adam",
    "save_dir": "saved_models",
    "layers": [
      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": true
      },
      {
        "type": "dropout",
        "rate": 0.2
      },
      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": true
      },
      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": false
      },
      {
        "type": "dropout",
        "rate": 0.2
      },
      {
        "type": "dense",
        "neurons": 1,
        "activation": "linear"
      }
    ]
  }
}
